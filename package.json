{
  "name": "serverless",
  "version": "0.0.1",
  "description": "Express-Serverless for building RESTful APIs",
  "author": "Jupser Team <info@jupser.com>",
  "private": true,
  "main": "App.ts",
  "scripts": {
    "precommit": "yarn lint",
    "start": "sls offline start --skipCacheInvalidation",
    "docs": "apidoc -i src -o docs",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "deploy:staging": "sls deploy --stage staging",
    "deploy:production": "sls deploy --stage production"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "serverless-domain-manager": "^3.2.6",
    "serverless-http": "^2.0.2",
    "serverless-offline": "^5.5.0",
    "serverless-plugin-optimize": "^4.1.0-rc.1",
    "serverless-plugin-typescript": "^1.1.7",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0"
  }
}
